<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_assistant">

    <fragment
        android:id="@+id/navigation_assistant"
        android:name="com.hfut.edu.emotionalassistant.ui.assistant.AssistantFragment"
        android:label="@string/title_assistant"
        tools:layout="@layout/fragment_assistant" />

    <fragment
        android:id="@+id/navigation_statistics"
        android:name="com.hfut.edu.emotionalassistant.ui.statistics.StatisticsFragment"
        android:label="@string/title_statistics"
        tools:layout="@layout/fragment_statistics" />

    <fragment
        android:id="@+id/navigation_community"
        android:name="com.hfut.edu.emotionalassistant.ui.community.CommunityFragment"
        android:label="@string/title_community"
        tools:layout="@layout/fragment_community" />

    <fragment
        android:id="@+id/navigation_config"
        android:name="com.hfut.edu.emotionalassistant.ui.config.ConfigFragment"
        android:label="@string/title_config"
        tools:layout="@layout/fragment_config"></fragment>
</navigation>